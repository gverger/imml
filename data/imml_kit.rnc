#default namespace = "http://ns.immateriel.fr/imml"
namespace x = "http://www.w3.org/1999/xhtml" 

## book and order subschemas may be changed independently in the calling schema
## example : imml.book.schema.version |= "2.0"
imml.book.schema.version = notAllowed
imml.order.schema.version = notAllowed

## when an element is not supported at the emitter end, it must have
## an unsupported attribute with "true" value. Otherwise, it may explicitly 
## indicate support with a "false" value, but absence of this att is ok too.
imml.unsupported = attribute unsupported { "true" } & anyContent
imml.not_unsupported = attribute unsupported { "false" }? 

start = 
   element imml { 
      imml.message.attributes
    & imml.message.header?
    & imml.message.content
}

imml.message.attributes = 
   attribute timestamp { xsd:dateTime }?

############ 
## Header ##
############
imml.message.header =
   element header {
      attribute sender-id { imml.message.sender.id.datatype }
      ## message-id may typically contain a timestamp
    & attribute message-id { imml.message.id.datatype }
   }


imml.message.content =
   imml.book
 | imml.order

##########
## Book ##
##########

imml.book =
   # the version attribute is a child of <imml>
   attribute version { imml.book.schema.version }
 & element book {
      attribute ean { imml.book.ean.datatype }
    & attribute uid { imml.book.id.datatype }
    & imml.book.metadata
    & imml.book.assets
    & imml.book.offer
   }
####
# Book metadata
####
imml.book.metadata =
   element metadata {
      imml.book.metadata.title
    & imml.book.metadata.subtitle?
    & imml.book.metadata.collection?
    & imml.book.metadata.language
    & imml.book.metadata.publisher
    & imml.book.metadata.contributors
    & imml.book.metadata.topics
    & imml.book.metadata.description
   }
# Book title
imml.book.metadata.title =
   element title { text }
# Book subtitle
imml.book.metadata.subtitle =
   element subtitle { text }
imml.book.metadata.publisher =
   element publisher {
      attribute uid {
         imml.book.metadata.publisher.id.datatype
      }
    & text
   }
# Book collection
imml.book.metadata.collection =
   element collection { 
      attribute uid { imml.book.metadata.collection.id.datatype }?
    & text
   }
# Book language
imml.book.metadata.language =
   element language { imml.book.metadata.language.datatype }
 | attribute lang { imml.book.metadata.language.datatype }

# Book contributors
imml.book.metadata.contributors =
   element contributors {
      imml.book.metadata.contributor+
   }
imml.book.metadata.contributor =
   element contributor {
      imml.book.metadata.contributor.uid
    & imml.book.metadata.contributor.name
    & imml.book.metadata.contributor.role
   }
imml.book.metadata.contributor.uid =
   attribute uid {
      imml.book.contributor.id.datatype
   }
imml.book.metadata.contributor.name =
   element name { text }
imml.book.metadata.contributor.role =
   element role {
      imml.unsupported | ( imml.not_unsupported & imml.book.contributor.datatype )
   }
 
# Book topics
imml.book.metadata.topics =
   element topics {
      imml.unsupported | ( imml.not_unsupported & imml.book.metadata.topic+ )
   }
imml.book.metadata.topic =
   element topic {
      imml.book.metadata.topic.class
   }
imml.book.metadata.topic.class =
      imml.book.metadata.topic.bisac
    | imml.book.metadata.topic.clil
    | imml.book.metadata.topic.keyword

imml.book.metadata.topic.bisac =
   attribute type { "bisac" }
 & ( imml.unsupported | ( imml.not_unsupported & imml.bisac.id.datatype ) )

imml.book.metadata.topic.clil =
   attribute type { "clil" }
 & ( imml.unsupported | ( imml.not_unsupported & imml.clil.id.datatype ) )

imml.book.metadata.topic.keyword =
   attribute type { "keyword" }
 & ( imml.unsupported | ( imml.not_unsupported & text ) )

# Book description

imml.book.metadata.description.attributes =
   ## Default to book language (although ideally bookseller preferred language)
   attribute lang { imml.book.metadata.language.datatype }?
 & attribute extent { "short" | "long" | "140" }?

imml.book.metadata.description =
   element description {
      # imml.book.metadata.description.attributes &
      imml.book.metadata.description.content
   }

imml.book.metadata.description.content =
   imml.book.metadata.description.content.xhtml 
 | imml.book.metadata.description.content.text

imml.book.metadata.description.content.xhtml =
   attribute format { "xhtml" }
   & ( imml.unsupported | 
      ( imml.not_unsupported & anyXHTMLElement ) ) 

imml.book.metadata.description.content.text =
   text

###
# Book assets
###
imml.book.assets =
   element assets {
      imml.book.assets.cover
    & imml.book.assets.extract*
    & imml.book.assets.full*
   }
## Cover files are made available to the receiving reseller via an ad hoc url
imml.book.assets.cover =
   element cover {
      imml.book.assets.url
    & imml.book.assets.mimetype
   }

## Extract files are made available to the receiving reseller via an ad hoc url
imml.book.assets.extract =
   element extract {
      imml.unsupported | ( imml.not_unsupported & imml.book.assets.extract.content )
   }
imml.book.assets.extract.content =
      imml.book.assets.url
    & imml.book.assets.mimetype

## Content files are made available via an ad hoc url tailored for the receiving warehouse
imml.book.assets.full =
   element full {
      imml.unsupported | ( imml.not_unsupported & imml.book.assets.full.content )
   }
imml.book.assets.full.content =
      imml.book.assets.url
    & imml.book.assets.mimetype

imml.book.assets.mimetype = 
   attribute mimetype {
      imml.book.assets.mimetype.datatype
   }
imml.book.assets.url = 
   attribute url {
      imml.book.assets.url.datatype
   }
###
# Book offer
###
imml.book.offer =
   element offer {
      # =ex-support
      imml.book.offer.medium?
    & imml.book.offer.manifestation?
    & imml.book.offer.pagination?
    & imml.book.offer.alternatives?
      # if not ready for sale, allow price to be missing
    & ( (element ready_for_sale { "false" })
       |(element ready_for_sale { xsd:boolean } & imml.book.offer.prices ) )
    & imml.book.offer.sales_start_at
   }
## Book medium (for now, printed|digital)
imml.book.offer.medium =
   element medium {
      imml.book.offer.medium.datatype
   }
## Manifestation
imml.book.offer.manifestation =
   element format {
      imml.book.offer.manifestation.datatype
   }
## Pagination of whatever alternative format where it makes sense
imml.book.offer.pagination =
   element pagination {
      imml.book.offer.pagination.datatype
   }
## Alternative offers for the same content
imml.book.offer.alternatives = 
   element alternatives {
      imml.book.offer.alternative+
   }
imml.book.offer.alternative =
   element alternative {
      attribute ean { imml.book.ean.datatype }
    & attribute medium { imml.book.offer.medium.datatype }?
    & attribute format { imml.book.offer.manifestation.datatype }?
   }
## Date d'effet
imml.book.offer.sales_start_at =
   element sales_start_at {
      imml.unsupported | ( imml.not_unsupported & xsd:date )
   }
## Book prices
imml.book.offer.prices = 
   element prices {
      imml.book.offer.price+
   }
imml.book.offer.price =
   element price {
      imml.book.offer.price.currency
    & imml.book.offer.price.current_amount
    & imml.book.offer.price.territories
   }

# Currency
imml.book.offer.price.currency =
   attribute currency {
      imml.book.offer.price.currency.datatype
   }
# Public price amount 
imml.book.offer.price.current_amount =
   element current_amount { 
      imml.book.offer.price.datatype 
   }
## Book territories
imml.book.offer.price.territories =
   element territories {
      imml.book.offer.territory.datatype
   }

###########
## Order ##
###########
# As of version 0, nothing interesting
imml.order = 
   # the version attribute is a child of <imml>
   attribute version { imml.order.schema.version }
 & element order {
      anyContent
   } 

########################################
## Tools for expressing "whatever child"
## See http://www.relaxng.org/compact-tutorial-20030326.html#id2816343
anyElement =
  element * {
    (attribute * { text }
     | text
     | anyElement)*
  }
anyXHTMLElement =
  element x:* { 
     (attribute * { text }
      | text
      | anyXHTMLElement)*
  }
anyContent =
   (attribute * { text }
    | text
    | anyElement)*

###############
## Datatypes ##
###############
imml.book.assets.url.datatype = xsd:anyURI
imml.book.assets.mimetype.datatype = 
   "image/jpeg" 
 | "image/png" 
 | "audio/mpeg"
 | "video/mpeg"
 | "video/mp4"
 | "video/x-flv"
 | "application/pdf" 
 | "text/html" 
 | "application/zip" 
 | "application/x-mobipocket" 
 | "application/x-mobipocket-ebook" 
 | "application/x-rar-compressed" 
 | "application/epub+zip"

imml.book.contributor.datatype = "author" | "translator" | "editor" | "typesetter"
imml.bisac.id.datatype = xsd:token { pattern="[A-Z]{3}[0-9]{6}" } 
imml.clil.id.datatype = xsd:token { pattern="[0-9]{4}" } 
imml.book.offer.medium.datatype = "printed" | "digital"
imml.book.offer.manifestation.datatype = "epub" | "pdf" | "streaming" | "bundle"
imml.book.offer.pagination.datatype = xsd:integer { minInclusive="0" }
imml.book.offer.price.datatype =  xsd:decimal { minInclusive="0" fractionDigits="2" }
imml.book.metadata.language.datatype = list { List74+ }
imml.uid.datatype = xsd:token { pattern="C[A-Z]{1}[0-9]+" }
imml.message.sender.id.datatype = xsd:NMTOKEN
imml.message.id.datatype = xsd:NMTOKEN
imml.book.id.datatype = xsd:token { pattern="C[B][0-9]+" }
imml.book.metadata.publisher.id.datatype = xsd:token { pattern="C[P][0-9]+" }
imml.book.metadata.collection.id.datatype = xsd:token { pattern="C[C][0-9]+" }
imml.book.contributor.id.datatype = imml.uid.datatype 
imml.book.ean.datatype = xsd:token { length="13" pattern="3[0-9]{12}|97[8-9][0-9]{10}" } 

imml.book.offer.territory.datatype = "WORLD" | imml.book.offer.country.datatype 
